@model IEnumerable<Dealership.Web.Models.CarViewModels.CarSummaryViewModel>

<section>
    @if (Model.Count() == 0)
    {
        <div class="col-md-4">
            <p>There are no cars :(</p>
        </div>
    }
    else
    {
        @foreach (var car in Model)
        {
            <partial name="_CarSummary" model="@car" />
        }
    }
</section>
